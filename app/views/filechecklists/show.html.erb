<div class="container">
    <div class="row">
        <div class="col m12">
            <div class="card">
                <div class="card-content">
                    <div class="page-header">
                        <h1>DOCUMENTOS</h1>
                        <%= form_for [@entry, @checklist] do |f| %>
                            <div class="input-field">
                                <%= f.check_box :referralSheet, :disabled => true  %>
                                <%= f.label 'referralSheet', "Hoja de Remisión" %>
                            </div>
                            <div class="input-field">
                                <%= f.check_box :producersList, :disabled => true  %>
                                <%= f.label 'producersList', "Lista de productores" %>
                            </div>
                            <div class="input-field">
                                <%= f.check_box :inspectionSheet, :disabled => true  %>
                                <%= f.label 'inspectionSheet', "Hoja de Inspección y limpieza del vehículo" %>
                            </div>
                            <div class="input-field">
                                <%= f.check_box :embacingControl, :disabled => true  %>
                                <%= f.label 'embacingControl', "Control del envase de producto" %>
                            </div>
                            <div class="input-field">
                                <%= f.check_box :collectionCleaningControl, :disabled => true  %>
                                <%= f.label 'collectionCleaningControl', "Control de limpieza de áreas del centro de acopio" %>
                            </div>
                            <div class="input-field">
                                <%= f.check_box :warehouseEntrySheet, :disabled => true  %>
                                <%= f.label 'warehouseEntrySheet', "Hoja de ingreso a bodega" %>
                            </div>
                            <div class="input-field">
                                <%= f.check_box :billCopy, :disabled => true  %>
                                <%= f.label 'billCopy', "Copia de factura" %>
                            </div>
                            <br>        
                            <div>
                                <%= link_to "Regresar",  :back , class: "btn btn-primary"%>
                            </div>
                        <% end %>             
                    </div>
                </div>
            </div>
        </div>
    </div>      
</div>